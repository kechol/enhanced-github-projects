{
  "name": "Enhanced GitHub Projects",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A chrome extension for better GitHub Projects",
  "homepage_url": "https://github.com/kechol/enhanced-github-projects",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://github.com/*"
  ],
  "content_scripts": [{
    "matches": [
      "https://github.com/*"
    ],
    "js": [
      "dist/inject.js"
    ]
  }]
}
